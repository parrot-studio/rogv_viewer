<h3>Call Ranking</h3>
<table class="table table-striped  table-condensed">
  <thead>
    <tr>
      <th class="span1">Rank</th>
      <th class="span3">Guild</th>
      <th class="span1">Total</th>
    </tr>
  </thead>
  <tbody>
    <% each_rankers_for_view(@result, 10) do |vrank, gname, count| %>
      <tr>
        <td><%= vrank %></td>
        <td><%=h gname %></td>
        <td><%= count %></td>
      </tr>
    <% end %>
    <tr>
      <td colspan="3"><%= link_to('more...', url_for(:call, @result.gv_date)) %></td>
    </tr>
  </tbody>
</table>
